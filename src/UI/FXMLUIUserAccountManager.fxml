<AnchorPane id="AnchorPane" prefHeight="400.0" prefWidth="600.0" style="-fx-background-color: #00B926;" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="UI.FXMLUIUserAccountManagerController">
   <children>
      <GridPane alignment="CENTER" layoutX="150.0" layoutY="17.0" prefHeight="367.0" prefWidth="325.0">
        <columnConstraints>
          <ColumnConstraints hgrow="SOMETIMES" maxWidth="158.0" minWidth="10.0" prefWidth="106.0" />
          <ColumnConstraints hgrow="SOMETIMES" maxWidth="222.0" minWidth="10.0" prefWidth="219.0" />
        </columnConstraints>
        <rowConstraints>
          <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
          <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
            <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
            <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
            <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
        </rowConstraints>
         <children>
            <Button fx:id="Validate" mnemonicParsing="false" onAction="#click_validate" text="Validate" GridPane.halignment="CENTER" GridPane.rowIndex="4" />
            <Label text="Last name :" textFill="#d6d6d6" GridPane.halignment="CENTER" GridPane.rowIndex="1">
               <font>
                  <Font name="Apple SD GothicNeo ExtraBold" size="13.0" />
               </font>
            </Label>
            <Label text="First name :" textFill="#d6d6d6" GridPane.halignment="CENTER">
               <font>
                  <Font name="Apple SD GothicNeo ExtraBold" size="13.0" />
               </font>
            </Label>
            <Label text="Height (cm) : " textFill="#d6d6d6" GridPane.halignment="CENTER" GridPane.rowIndex="2">
               <font>
                  <Font name="Apple SD GothicNeo ExtraBold" size="13.0" />
               </font>
            </Label>
            <Label text="Weight :" textFill="#d6d6d6" GridPane.halignment="CENTER" GridPane.rowIndex="3">
               <font>
                  <Font name="Apple SD GothicNeo ExtraBold" size="13.0" />
               </font>
            </Label>
            <TextField fx:id="firstName_textField" GridPane.columnIndex="1" />
            <TextField fx:id="lastName_textField" GridPane.columnIndex="1" GridPane.rowIndex="1" />
            <TextField fx:id="height_textField" GridPane.columnIndex="1" GridPane.rowIndex="2" />
            <TextField fx:id="weight_textField" GridPane.columnIndex="1" GridPane.rowIndex="3" />
            <Button fx:id="changePassword_button" mnemonicParsing="false" text="Change password" GridPane.columnIndex="1" GridPane.halignment="CENTER" GridPane.rowIndex="4" />
         </children>
      </GridPane>
      <Label fx:id="labelText" />
      <Button fx:id="backButton" layoutX="25.0" layoutY="27.0" mnemonicParsing="false" onAction="#backClicked" text="Back" />
   </children>
</AnchorPane>
